{
    "label": "Components Library",
    "nav": [
        {
            "label": "Home",
            "id": "Home",
            "href": "/"
        },
        {
            "label": "Components",
            "id": "Components",
            "href": "/components"
        }
    ],
    "groups": [
        {
            "label": "Charts",
            "id": "charts",
            "path": "/charts",
            "description": "A collection of visualisations",
            "components": [
                {
                    "name": "BarChart",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any[]",
                            "default": "[]",
                            "example": [
                                {
                                    "name": "A",
                                    "value": 10
                                },
                                {
                                    "name": "B",
                                    "value": 20
                                }
                            ]
                        },
                        {
                            "prop": "title",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Bar Chart"
                        },
                        {
                            "prop": "xLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "X Axis"
                        },
                        {
                            "prop": "yLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Y Axis"
                        },
                        {
                            "prop": "accessor",
                            "type": "{ x: string, y: string }",
                            "default": "{ \"x\": \"\", \"y\": \"\" }",
                            "example": {
                                "x": "name",
                                "y": "value"
                            }
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"800\"",
                            "example": "800"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"400\"",
                            "example": "400"
                        }
                    ]
                },
                {
                    "name": "Gauge",
                    "props": [
                        {
                            "prop": "value",
                            "type": "number",
                            "default": "71",
                            "example": 50
                        },
                        {
                            "prop": "minValue",
                            "type": "number",
                            "default": "1",
                            "example": 0
                        },
                        {
                            "prop": "maxValue",
                            "type": "number",
                            "default": "100",
                            "example": 100
                        },
                        {
                            "prop": "size",
                            "type": "string",
                            "default": "\"200\"",
                            "example": "300"
                        },
                        {
                            "prop": "innerColor",
                            "type": "string",
                            "default": "\"accent\"",
                            "example": "accent"
                        },
                        {
                            "prop": "outerColor",
                            "type": "string",
                            "default": "\"neutral\"",
                            "example": "neutral"
                        },
                        {
                            "prop": "textColor",
                            "type": "string",
                            "default": "\"text\"",
                            "example": "text"
                        },
                        {
                            "prop": "label",
                            "type": "string",
                            "default": "\"Gauge\"",
                            "example": "Performance"
                        }
                    ]
                },
                {
                    "name": "Heatmap",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any[]",
                            "default": "[]",
                            "example": [
                                {
                                    "x": "A",
                                    "y": "1",
                                    "value": 10
                                },
                                {
                                    "x": "A",
                                    "y": "2",
                                    "value": 20
                                },
                                {
                                    "x": "B",
                                    "y": "1",
                                    "value": 30
                                },
                                {
                                    "x": "B",
                                    "y": "2",
                                    "value": 40
                                }
                            ]
                        },
                        {
                            "prop": "xLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "X Axis"
                        },
                        {
                            "prop": "yLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Y Axis"
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"800\"",
                            "example": "800"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"600\"",
                            "example": "600"
                        }
                    ]
                },
                {
                    "name": "LineChart",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any[]",
                            "default": "[]",
                            "example": [
                                {
                                    "time": "2021-01",
                                    "value": 30
                                },
                                {
                                    "time": "2021-02",
                                    "value": 45
                                }
                            ]
                        },
                        {
                            "prop": "title",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Monthly Sales"
                        },
                        {
                            "prop": "xLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Month"
                        },
                        {
                            "prop": "yLabel",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Sales"
                        },
                        {
                            "prop": "accessor",
                            "type": "{ x: string, y: string }",
                            "default": "{ \"x\": \"\", \"y\": \"\" }",
                            "example": {
                                "x": "time",
                                "y": "value"
                            }
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"800\"",
                            "example": "800"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"400\"",
                            "example": "400"
                        }
                    ]
                },
                {
                    "name": "Map",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any[]",
                            "default": "[]",
                            "example": "https://raw.githubusercontent.com/holtzy/D3-graph-gallery/master/DATA/world.geojson"
                        },
                        {
                            "prop": "label",
                            "type": "strMaping",
                            "default": "\"\"",
                            "example": "Locations"
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"800\"",
                            "example": "800"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"600\"",
                            "example": "600"
                        }
                    ]
                },
                {
                    "name": "PieChart",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any[]",
                            "default": "[]",
                            "example": [
                                {
                                    "label": "Apples",
                                    "value": 40
                                },
                                {
                                    "label": "Oranges",
                                    "value": 30
                                }
                            ]
                        },
                        {
                            "prop": "title",
                            "type": "string",
                            "default": "\"\"",
                            "example": "Fruit Distribution"
                        },
                        {
                            "prop": "accessor",
                            "type": "{ value: string; label: string }",
                            "default": "{ \"value\": \"\", \"label\": \"\" }",
                            "example": {
                                "value": "value",
                                "label": "label"
                            }
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"400\"",
                            "example": "400"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"400\"",
                            "example": "400"
                        }
                    ]
                },
                {
                    "name": "TreeView",
                    "props": [
                        {
                            "prop": "data",
                            "type": "any",
                            "default": "{}",
                            "example": {
                                "name": "Root",
                                "children": [
                                    {
                                        "name": "Child 1"
                                    },
                                    {
                                        "name": "Child 2"
                                    }
                                ]
                            }
                        },
                        {
                            "prop": "width",
                            "type": "string",
                            "default": "\"800\"",
                            "example": "800"
                        },
                        {
                            "prop": "height",
                            "type": "string",
                            "default": "\"600\"",
                            "example": "600"
                        }
                    ]
                }
            ]
        }
    ]
}